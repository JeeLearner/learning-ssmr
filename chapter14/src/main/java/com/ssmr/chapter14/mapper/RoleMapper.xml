<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssmr.chapter14.dao.RoleDao">
	<select id="getRole" parameterType="long" resultType="com.ssmr.chapter14.pojo.Role">
